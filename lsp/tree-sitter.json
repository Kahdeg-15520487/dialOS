{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dialscript",
      "camelcase": "Dialscript",
      "title": "Dial script",
      "scope": "source.dialscript",
      "file-types": [
        "dosc"
      ],
      "injection-regex": "^dialscript$",
      "class-name": "TreeSitterDialscript"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "script to run in DialOS on M5Dial",
    "authors": [
      {
        "name": "kahdeg"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-dialscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}