// Test compile-time function validation

// User-defined functions
function greet(name: string): string {
    return "Hello, " + name + "!";
}

function calculate(a: int, b: int): int {
    return a * 2 + b;
}

// Test valid calls
os.console.println("Testing function validation");

// Valid user function call
var greeting: greet("Alice");
os.console.println(greeting);

// Valid user function with math
var result: calculate(5, 10);
os.console.println("Result: " + result);

// Valid native call
os.console.println("All tests passed!");
