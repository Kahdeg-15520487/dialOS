class Coordinate{
    x: int;
    y: int;

    constructor(x:int,y:int){
        assign this.x x;
        assign this.y y;
    }

    toString(): string{
        return `(${this.x}:${this.y})`;
    }
}

var p1: Coordinate(80,100);
var p2: Coordinate(100,80);

os.console.println(p1.toString());
os.console.println(p2.toString());